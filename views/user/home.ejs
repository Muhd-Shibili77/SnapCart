    <script>
        async function addToCart(product_id, variant_id) {
            const quantity = 1
            try {
                const response = await fetch(`/cart/addToCart`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ product_id, variant_id, quantity })
                });

                const result = await response.json();

                if (result.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Added to cart',
                        showConfirmButton: false,
                        text: result.message,

                    }).then(() => {
                        location.reload();
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: result.message,
                        showConfirmButton: true
                    }).then(() => {
                        location.reload();
                    });
                }
            } catch (error) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An error occurred while processing your request.',
                    showConfirmButton: true
                });
            }
        }

        async function addToWishlist(productId, variantId) {


            try {
                const response = await fetch(`/wishlist/toWishlist`, {
                    method: "POST",
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId, variantId })
                })
                const result = await response.json()

                if (result.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Added to wishlist',
                        showConfirmButton: true,
                        text: result.message,
                    })

                } else {
                    if (result.error == "product already in  wishlist") {
                        Swal.fire({
                            icon: 'info',
                            title: 'Already Added',
                            text: result.error,
                            showConfirmButton: true
                        })
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: result.error,
                            showConfirmButton: true
                        })
                    }

                }

            } catch (error) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An error occurred while processing your request.',
                    showConfirmButton: true
                });
            }
        }
    </script>




