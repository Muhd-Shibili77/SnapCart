<%- include ("../partial/header" )-%>
    <style>
        .original-price {
            color: red !important;
            /* Sets the text color to red */
            font-size: 15px !important;
            text-decoration: line-through;
            /* Adds a strikethrough effect */
            margin-right: 5px;
            /* Adds space between original and offer price */
        }

        .thumb {
            position: relative;
        }

        .offer-badge {
            position: absolute;
            top: 10px;
            right: 10px;
            color: white;
            border-radius: 50%;           
            z-index: 1;
            
        }
