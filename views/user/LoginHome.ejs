<%- include ("../partial/header")-%>
  <!-- ***** Header Area Start ***** -->
  <header class="header-area header-sticky">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <nav class="main-nav">
                    <!-- ***** Logo Start ***** -->
                    <a href="#" class="logo">
                        <img src="/images/SnapCart.png">
                    </a>
                    <!-- ***** Logo End ***** -->
                    <!-- ***** Menu Start ***** -->
                    <ul class="nav">
                        
                        <li class="scroll-to-section"><a href="#top" class="active">Home</a></li>
                        <li class="scroll-to-section"><a href="#men">Men's</a></li>
                        <li class="scroll-to-section"><a href="#women">Women's</a></li>
                        <li class="scroll-to-section"><a href="#kids">Kid's</a></li>
                        <li class="scroll-to-section"><a href="/user/login">Products</a></li>
                        <li class="scroll-to-section"><a href="/user/login">Login</a></li>
                        <li class="scroll-to-section"><a href="/user/signup">Signup</a></li>
                      
                    </ul>        
                    <a class='menu-trigger'>
                        <span>Menu</span>
                    </a>
                    
                </nav>
            </div>
        </div>
    </div>
</header>
<!-- ***** Header Area End ***** -->
 <!-- ***** Main Banner Area Start ***** -->
 <div class="main-banner" id="top">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-6">
                <div class="left-content">
                    <div class="thumb">
                        <div class="inner-content">
                            <h4>Find your perfect fit
                                where style meets
                                confidence</h4>
                            
                            <div class="main-border-button">
                                <a href="/user/login">Purchase Now!</a>
                            </div>
                        </div>
                        <img src="/images/left-banner-image.jpg" alt="">
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="right-content">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="right-first-image">
                                <div class="thumb">
                                    <div class="inner-content">
                                        <h4>Women</h4>
                                        <span>Best Clothes For Women</span>
                                    </div>
                                    <div class="hover-content">
                                        <div class="inner">
                                            <h4>Women</h4>
                                            <p>Elevate your wardrobe with our women's fashion collection, featuring chic and versatile pieces designed to empower your unique style</p>
                                            <div class="main-border-button">
                                                <a href="/user/login">View More</a>
                                            </div>
                                        </div>
                                    </div>
                                    <img src="/images/baner-right-image-01.jpg">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="right-first-image">
                                <div class="thumb">
                                    <div class="inner-content">
                                        <h4>Men</h4>
                                        <span>Best Clothes For Men</span>
                                    </div>
                                    <div class="hover-content">
                                        <div class="inner">
                                            <h4>Men</h4>
                                            <p>Step up your style game with our men's fashion selection, offering modern and classic pieces that redefine sophistication and comfort</p>
                                            <div class="main-border-button">
                                                <a href="/user/login">View More</a>
                                            </div>
                                        </div>
                                    </div>
                                    <img src="/images/baner-right-image-02.jpg">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="right-first-image">
                                <div class="thumb">
                                    <div class="inner-content">
                                        <h4>Kids</h4>
                                        <span>Best Clothes For Kids</span>
                                    </div>
                                    <div class="hover-content">
                                        <div class="inner">
                                            <h4>Kids</h4>
                                            <p>Discover the latest in kids' fashion with playful and comfortable clothing that combines style and fun for every little trendsetter.</p>
                                            <div class="main-border-button">
                                                <a href="/user/login">View More</a>
                                            </div>
                                        </div>
                                    </div>
                                    <img src="/images/baner-right-image-03.jpg">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="right-first-image">
                                <div class="thumb">
                                    <div class="inner-content">
                                        <h4>Accessories</h4>
                                        <span>Best Trend Accessories</span>
                                    </div>
                                    <div class="hover-content">
                                        <div class="inner">
                                            <h4>Accessories</h4>
                                            <p>Explore our collection of stylish accessories, including shoes, belts, and more, to complete your look with the perfect finishing touches</p>
                                            <div class="main-border-button">
                                                <a href="/user/login">View More</a>
                                            </div>
                                        </div>
                                    </div>
                                    <img src="/images/baner-right-image-04.jpg">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ***** Main Banner Area End ***** -->
  <!-- ***** Men Area Starts ***** -->
  <section class="section" id="men">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="section-heading">
                    <h2>Men's Latest</h2>
                   
                </div>
            </div>
            <div class="col-lg-6 d-flex justify-content-end">
                <div class="section-heading">
                   
                    <a href="/user/login" class="btn btn-outline-dark">View More</a>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="men-item-carousel">
                    <div class="owl-men-item owl-carousel">
                        <% mensProduct.forEach((element)=> { %>
                        <div class="item">
                            <div class="thumb">
                                
                                <a href="/user/login"> <img src="/<%= element.variants[0].images[0] %>" alt=""></a>
                            </div>
                            <div class="down-content">
                                <h4><%= element.product_name %></h4>
                                <span>INR <%= element.variants[0].price %></span>
                                <ul class="stars">
                                    <li><i class="bi bi-star-fill"></i></li>
                                    <li><i class="bi bi-star-fill"></i></li>
                                    <li><i class="bi bi-star-fill"></i></li>
                                    <li><i class="bi bi-star-fill"></i></li>
                                    <li><i class="bi bi-star-fill"></i></li>
                                </ul>
                            </div>
                        </div>
                    <% }); %>
                       
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ***** Men Area Ends ***** -->
